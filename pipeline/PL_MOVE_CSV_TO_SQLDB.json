{
	"name": "PL_MOVE_CSV_TO_SQLDB",
	"properties": {
		"activities": [
			{
				"name": "Copy CSV",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
